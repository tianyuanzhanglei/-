<view class='img'>
  <view class='img_title'>照片</view>
  <view class='img_head'><image class='img_photo' src="{{stu.img}}"></image></view>
</view>
<view class='infor'>
  <view class='infor_left'>
    <view class='infor_title'>姓名</view>
    <view class='infor_data'>{{stu.name}}</view>
  </view>
  <view class='infor_right'>
    <view class='infor_title'>性别</view>
    <view class='infor_data'>{{stu.sex}}</view>
  </view>
</view>
<view class='infor'>
  <view class='infor_left'>
    <view class='infor_title'>班级</view>
    <view class='infor_data'>{{stu.className}}</view>
  </view>
  <view class='infor_right'>
    <view class='infor_title'>学号</view>
    <view class='infor_data'>{{stu.stuId}}</view>
  </view>
</view>
<view class='infor'>
  <view class='infor_left_title'>宿舍号</view>
  <view class='infor_right_data'>{{stu.dormitoryId}}</view>  
</view>
<view class='infor'>
  <view class='infor_left_title'>联系电话</view>
  <view class='infor_right_data'>{{stu.tel}}</view>  
</view>
<view class='infor'>
  <view class='infor_left_title'>贫困等级</view>
  <view class='infor_right_data'>{{stu.grade}}</view>  
</view>
<view class='infor'>
  <view class='infor_left_title'>本人工行卡</view>
  <view class='infor_right_data'>{{stu.gardNumber}}</view>  
</view>
<view class='infor'>
  <view class='infor_left_title'>班级负责人</view>
  <view class='infor_right_data'>{{stu.monitor}}</view>  
</view>